[user]
  name        = wwwins
  email       = wwwins@aaa.bbb.ccc
[push]
  default     = simple
[help]
  autocorrect = 1
[apply]
  whitespace  = nowarn
[color]
  ui          = auto
  branch      = auto
  status      = auto
  diff        = auto
[core]
  autocrlf    = input
  safecrlf    = false
[alias]
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
